  <xsd:complexType ecore:name="TC74Sample" name="TC74_Sample">
    <xsd:annotation>
      <xsd:documentation>Microchip TC74
Tiny Serial Digital Thermal Sensor

http://ww1.microchip.com/downloads/en/devicedoc/21462c.pdf</xsd:documentation>
