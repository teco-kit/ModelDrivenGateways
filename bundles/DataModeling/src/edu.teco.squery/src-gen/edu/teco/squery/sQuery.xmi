<?xml version="1.0" encoding="ASCII"?>
<xtext:XtextFile xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xtext="http://www.openarchitectureware.org/xtext" name="squery" nsURI="http://www.teco.edu/squery">
  <mmImports location="http://www.teco.edu/automata" alias="automataMM"/>
  <rules xsi:type="xtext:TypeRule" name="QueryModel">
    <type alias="" name="QueryModel"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Assignment" feature="automata" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="Automata"/>
      </children>
      <children xsi:type="xtext:Assignment" cardinality="ONEORMORE" feature="actions" operator="ADD">
        <token xsi:type="xtext:RuleName" name="Action"/>
      </children>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="Automata">
    <type alias="" name="Automata"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Keyword" value="model"/>
      <children xsi:type="xtext:Assignment" feature="model" operator="ASSIGN">
        <token xsi:type="xtext:FileRef"/>
      </children>
      <children xsi:type="xtext:Keyword" value=";"/>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="Action">
    <type alias="" name="Action"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Keyword" value="action"/>
      <children xsi:type="xtext:Assignment" feature="name" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="ID"/>
      </children>
      <children xsi:type="xtext:Keyword" value="on"/>
      <children xsi:type="xtext:Assignment" feature="query" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="Query"/>
      </children>
      <children xsi:type="xtext:Keyword" value=";"/>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="Query">
    <type alias="" name="Query"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Assignment" feature="st" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="State"/>
      </children>
      <children xsi:type="xtext:Assignment" cardinality="OPTIONAL" feature="condition" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="Condition"/>
      </children>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="Condition">
    <type alias="" name="Condition"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Keyword" value="where"/>
      <children xsi:type="xtext:Assignment" feature="expressions" operator="ADD">
        <token xsi:type="xtext:RuleName" name="BoolExpression"/>
      </children>
      <children xsi:type="xtext:Group" cardinality="ANY">
        <children xsi:type="xtext:Alternatives">
          <alternatives xsi:type="xtext:Keyword" value="||"/>
          <alternatives xsi:type="xtext:Keyword" value="&amp;&amp;"/>
        </children>
        <children xsi:type="xtext:Assignment" feature="expressions" operator="ADD">
          <token xsi:type="xtext:RuleName" name="BoolExpression"/>
        </children>
      </children>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="BoolExpression">
    <type alias="" name="BoolExpression"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Assignment" feature="op1" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="State"/>
      </children>
      <children xsi:type="xtext:Assignment" feature="operand" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="Operand"/>
      </children>
      <children xsi:type="xtext:Assignment" feature="op2" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="STRING"/>
      </children>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="State">
    <type alias="" name="State"/>
    <content xsi:type="xtext:Alternatives">
      <alternatives xsi:type="xtext:Assignment" feature="state" operator="ADD">
        <token xsi:type="xtext:CrossReference">
          <type alias="automataMM" name="SimpleState"/>
        </token>
      </alternatives>
      <alternatives xsi:type="xtext:Group" cardinality="ONEORMORE">
        <children xsi:type="xtext:Keyword" value="/"/>
        <children xsi:type="xtext:Assignment" feature="state" operator="ADD">
          <token xsi:type="xtext:CrossReference">
            <type alias="automataMM" name="SimpleState"/>
          </token>
        </children>
      </alternatives>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="Operand">
    <type alias="" name="Operand"/>
    <content xsi:type="xtext:Alternatives">
      <alternatives xsi:type="xtext:Assignment" feature="op" operator="ASSIGN">
        <token xsi:type="xtext:Keyword" value="=="/>
      </alternatives>
      <alternatives xsi:type="xtext:Assignment" feature="op" operator="ASSIGN">
        <token xsi:type="xtext:Keyword" value="&lt;="/>
      </alternatives>
      <alternatives xsi:type="xtext:Assignment" feature="op" operator="ASSIGN">
        <token xsi:type="xtext:Keyword" value=">="/>
      </alternatives>
      <alternatives xsi:type="xtext:Assignment" feature="op" operator="ASSIGN">
        <token xsi:type="xtext:Keyword" value="&lt;"/>
      </alternatives>
      <alternatives xsi:type="xtext:Assignment" feature="op" operator="ASSIGN">
        <token xsi:type="xtext:Keyword" value=">"/>
      </alternatives>
    </content>
  </rules>
  <rules xsi:type="xtext:NativeLexerRule" name="ID"/>
  <rules xsi:type="xtext:NativeLexerRule" name="STRING"/>
  <rules xsi:type="xtext:NativeLexerRule" name="INT"/>
  <rules xsi:type="xtext:NativeLexerRule" name="WS"/>
  <rules xsi:type="xtext:NativeLexerRule" name="ML_COMMENT"/>
  <rules xsi:type="xtext:NativeLexerRule" name="SL_COMMENT"/>
</xtext:XtextFile>
