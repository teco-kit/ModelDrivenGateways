<wsdl:definitions xmlns:wsoap12="http://schemas.xmlsoap.org/wsdl/soap12/"
	xmlns:tns="http://www.teco.edu/SensorValues" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	
	xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/08/addressing" 
	xmlns:wse="http://schemas.xmlsoap.org/ws/2004/08/eventing"
	
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/"
    
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	
	name="SensorValues" targetNamespace="http://www.teco.edu/SensorValues"
	>

    <wsdl:import location="SensorValues.wsdl" namespace="http://www.teco.edu/SensorValues"/>	
	<wsdl:portType name="SensorValuesClientPort" wse:EventSource="true">
		
		<wsdl:operation name="SensorValuesEventSink">
			<wsdl:input message="tns:SensorValuesEventMessageOut"
				wsa:Action="http://www.teco.edu/SensorValues/SensorValuesEventOut" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="SensorValuesClientSOAP" type="tns:SensorValuesClientPort">
		<soap:binding style="document" 	transport="http://schemas.xmlsoap.org/soap/http" />
		
		<wsdl:operation name="SensorValuesEventSink">
			<soap:operation
				soapAction="http://www.teco.edu/SensorValues/SensorValuesEvent" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
		</wsdl:operation>
</wsdl:binding>

	<wsdl:service name="SensorValuesClient">
		<wsdl:port name="SensorValuesClientPort" binding="tns:SensorValuesSOAP">
			<wsoap12:address location="" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
